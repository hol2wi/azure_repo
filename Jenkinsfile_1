pipeline {
    agent {
    node {
        label 'SWSVW_AUTOSCRIPT'
        }
}
    stages {
        stage('Build') {
            steps {
                echo 'starting Build' 
            }
        }
        
     stage('Wait_10s') {
            steps {
                sleep(time: 10, unit: "SECONDS") 
            }
        }
       
        
        stage('Test') {
            steps {
                echo 'Starting Test' 
            }
        }
       
}
}